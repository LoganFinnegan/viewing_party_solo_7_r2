<%= button_to "Discover Top Rated Movies", user_movies_path(@user), method: :get, params: { top_rated: true } %>


<%= form_with url: user_movies_path(@user), method: :get, data: {turbo: false}, local: true do |f| %>
  <%= f.label :keyword, 'Movie Title' %>
  <%= f.text_field :keyword %>

  <%= f.submit "Search by Movie Title"%>
<% end %>